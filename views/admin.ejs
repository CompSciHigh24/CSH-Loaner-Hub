    <!-- LOANER DATABASE DISPLAY -->
      <!-- This is where we display the database of all the loaner computers via postman -->

     <% for(let x = 0; x < student.length; x++){ %>
        <div class="card" style="width: 18rem;">
          <img src="/images/chrome3.jpg" class="card-img-top" alt="...">
          <div class="card-body">
            <h5 class="card-title"><%= student[x].name %></h5>
            <p class="card-text"><%= student[x].email %></p>
            <p class="card-text"><%= student[x].advisory %></p>
            <p class="card-text"><%= student[x].room %></p>
          </div>
        </div>
     <% } %>
